{
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,

	"files.eol": "\n",
	"files.encoding": "utf8",
	"files.insertFinalNewline": true,

	"files.exclude": {
		"*.log*": true,
		"coverage": true,
		"node_modules": true,
		"package-lock.json": true
	},

	"files.associations": {
		"*.cjs": "javascript"
	}
}
